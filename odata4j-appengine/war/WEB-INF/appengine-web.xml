<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
	<application>odata4j-sample</application>
	<version>2</version>
	<system-properties>
       <property name="odata4j.producerfactory" value="org.odata4j.appengine.DatastoreProducerFactory" />
       <property name="odata4j.datastore.namespace" value="Datastore" />
       <property name="odata4j.datastore.devkinds" value="Category,Customer,Employee,Order,Product,Region,Shipper,Supplier,Territory" />
       <property name="odata4j.datastore.prodkinds" value="Category,Customer,Employee,Order,Product,Region,Shipper,Supplier,Territory" />
    </system-properties>
</appengine-web-app>